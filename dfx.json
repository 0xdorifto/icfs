{
  "canisters": {
    "collection": {
      "candid": "src/collection/collection.did",
      "package": "collection",
      "type": "rust"
    },
    "management": {
      "candid": "src/management/management.did",
      "package": "management",
      "type": "rust"
    },
    "frontend": {
      "dependencies": ["management"],
      "source": ["src/frontend/dist"],
      "type": "assets",
      "build": "cd src/frontend && npm install && npm run build"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
