{
  "canisters": {
    "collection": {
      "candid": "src/collection/collection.did",
      "package": "collection",
      "type": "rust",
      "init_arg": "(record { owner = principal \"aaaaa-aa\"; name = \"My Collection\"; collection_size = 1000 : nat64; chain_name = \"IC\"; description = \"My NFT Collection\"; standard = \"ERC721\"; image_canister_id = principal \"aaaaa-aa\" })"
    },
    "management": {
      "candid": "src/management/management.did",
      "package": "management",
      "type": "rust"
    },
    "image": {
      "candid": "src/image/image.did",
      "package": "image",
      "type": "rust"
    },
    "frontend": {
      "dependencies": ["collection", "management", "image"],
      "source": ["src/frontend/dist"],
      "type": "assets",
      "workspace": "frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
