type Collection = record {
  owner : text;
  name : text;
  description : text;
  collection_size : nat64;
  image_canister_id : principal;
  chain_name : text;
  standard : text;
};
service : {
  add_collection : (text, Collection) -> (text);
  add_user : (text) -> (text);
  list_user_collections : (text) -> (opt vec Collection) query;
}
